{
  "name": "cmmi-specs-mcp-server",
  "version": "0.0.1",
  "description": "CMMI Specs MCP Server - Multi-agent execution orchestrator for GitHub Copilot",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "npx tsc || (npm install --no-save typescript && npx tsc)",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "bin": {
    "cmmi-specs-agent-server": "dist/server.js"
  },
  "keywords": [
    "mcp",
    "multi-agent",
    "orchestrator",
    "copilot",
    "ai",
    "automation"
  ],
  "author": "Jieky",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "js-yaml": "^4.1.0",
    "uuid": "^10.0.0",
    "openai": "^4.67.3",
    "@anthropic-ai/sdk": "^0.27.0",
    "zod": "^3.23.8",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "@types/jest": "^29.5.13",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
